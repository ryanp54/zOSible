---
mainframe:
  hosts:
    host:
      ansible_host: {{ ansible_host }}
      ansible_user: {{ ansible_user }}
      ansible_port: {{ ansible_port }}
{% if ansible_python_interpreter %}
      ansible_python_interpreter: "{{ python_interpreter }}"
{% endif %}
{% if ask_pass is defined %}
      ask_pass: {{ ask_pass }}
{% endif %}
{% if no_ssh is defined %}
      no_ssh: {{ no_ssh }}
{% endif %}